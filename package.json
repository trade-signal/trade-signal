{
  "name": "trade-signal",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run cron:dev && npm run frontend:dev",
    "frontend:dev": "next dev --turbopack",
    "cron:dev": "tsx cron/index.ts",
    "build": "npm run cron:build && npm run frontend:build",
    "frontend:build": "next build",
    "cron:build": "tsx cron/index.ts --outDir .cron",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mantine/core": "^7.14.3",
    "@mantine/hooks": "^7.14.3",
    "@mantine/spotlight": "^7.15.0",
    "@prisma/client": "^6.0.1",
    "@tabler/icons-react": "^3.24.0",
    "cron": "^2.3.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "next": "15.0.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5"
  }
}
