generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Stock {
  id       Int       @id @default(autoincrement())
  type     String // a,sh,sz
  code     String    @unique
  name     String?
  fullName String?
  date     DateTime?
  symbol   String?

  @@map("chives_stocks")
}
